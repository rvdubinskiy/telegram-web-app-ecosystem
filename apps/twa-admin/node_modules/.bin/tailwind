#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/tailwindcss@3.4.3_ts-node@10.9.1_@types+node@18.11.3_typescript@4.8.4_/node_modules/tailwindcss/lib/node_modules:/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/tailwindcss@3.4.3_ts-node@10.9.1_@types+node@18.11.3_typescript@4.8.4_/node_modules/tailwindcss/node_modules:/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/tailwindcss@3.4.3_ts-node@10.9.1_@types+node@18.11.3_typescript@4.8.4_/node_modules:/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/tailwindcss@3.4.3_ts-node@10.9.1_@types+node@18.11.3_typescript@4.8.4_/node_modules/tailwindcss/lib/node_modules:/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/tailwindcss@3.4.3_ts-node@10.9.1_@types+node@18.11.3_typescript@4.8.4_/node_modules/tailwindcss/node_modules:/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/tailwindcss@3.4.3_ts-node@10.9.1_@types+node@18.11.3_typescript@4.8.4_/node_modules:/Users/rvdubinskiy/PET/titscoin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
